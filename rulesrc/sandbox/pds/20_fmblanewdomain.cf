
# check_rbl_from_domain subtest implemented only from 3.4.1
if (version >= 3.004001)
ifplugin Mail::SpamAssassin::Plugin::DNSEval

header     T_FROM_FMBLA_NEWDOM    eval:check_rbl_from_domain('fmbla', 'bl.fmb.la.', '127.2.0.2')
describe   T_FROM_FMBLA_NEWDOM    From domain was registered in last 7 days
tflags     T_FROM_FMBLA_NEWDOM    net
score      T_FROM_FMBLA_NEWDOM    1.0
reuse      T_FROM_FMBLA_NEWDOM

header     T_FROM_FMBLA_NEWDOM14  eval:check_rbl_from_domain('fmbla', 'bl.fmb.la.', '127.2.0.14')
describe   T_FROM_FMBLA_NEWDOM14  From domain was registered in last 7-14 days
tflags     T_FROM_FMBLA_NEWDOM14  net
score      T_FROM_FMBLA_NEWDOM14  0.7
reuse      T_FROM_FMBLA_NEWDOM14

endif
endif

ifplugin Mail::SpamAssassin::Plugin::URIDNSBL

urirhssub  T_URIBL_FMBLA_NEWDOM    bl.fmb.la. A 127.2.0.2
body       T_URIBL_FMBLA_NEWDOM    eval:check_uridnsbl('T_URIBL_FMBLA_NEWDOM')
describe   T_URIBL_FMBLA_NEWDOM    Body contains domain which was registered in last 7 days
tflags     T_URIBL_FMBLA_NEWDOM    net
score      T_URIBL_FMBLA_NEWDOM    1.0
reuse      T_URIBL_FMBLA_NEWDOM

urirhssub  T_URIBL_FMBLA_NEWDOM14  bl.fmb.la. A 127.2.0.14
body       T_URIBL_FMBLA_NEWDOM14  eval:check_uridnsbl('T_URIBL_FMBLA_NEWDOM14')
describe   T_URIBL_FMBLA_NEWDOM14  Body contains domain which was registered in last 7-14 days
tflags     T_URIBL_FMBLA_NEWDOM14  net
score      T_URIBL_FMBLA_NEWDOM14  0.7
reuse      T_URIBL_FMBLA_NEWDOM14

endif

